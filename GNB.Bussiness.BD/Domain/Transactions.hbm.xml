<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="GNB.Bussiness.BD" namespace="GNB.Bussiness.BD.Domain">
  <class name="Transactions" table="Transactions"  dynamic-update="true" >
	  <id name="ID" type="System.Int32" column="pk_TransactionId">
		  <generator class="identity"/>
	  </id>
	  <property type="string" name="Sku" column="TransactionSku" />
      <property type="decimal" name="Amount" column="TransactionAmount" />
	  <property type="string" name="Currency" column="TransactionCurrency" />  
  </class>
</hibernate-mapping>